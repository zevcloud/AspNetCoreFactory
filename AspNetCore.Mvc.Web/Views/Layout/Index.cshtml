@{
    ViewBag.JSCSSVision = "1.0.0";
    var isHaveListMore = ViewBag.isHaveListMore == true ? true : false;
}
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title-权限管理系统</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="/lib/bootstrap/dist/css/bootstrap.min.css?v=@(ViewBag.JSCSSVision)" rel="stylesheet" />
    <link href="/lib/font-awesome/css/font-awesome.css?v=@(ViewBag.JSCSSVision)" rel="stylesheet" />
    <link href="/lib/dist/css/adminlte.min.css?v=@(ViewBag.JSCSSVision)" rel="stylesheet" />
    <link href="/lib/dist/css/skins/_all-skins.min.css?v=@(ViewBag.JSCSSVision)" rel="stylesheet" />
    <link href="/lib/common/css/style.css?v=@(ViewBag.JSCSSVision)" rel="stylesheet" />
    <script src="/lib/jQuery/jquery-1.9.1.min.js?ver=@(ViewBag.JSCSSVision )"></script>
    <script type="text/javascript" src="/lib/bootstrap/dist/js/bootstrap.min.js?ver=@(ViewBag.JSCSSVision)"></script>
    <script type="text/javascript" src="/lib/dist/js/app.min.js?ver=@(ViewBag.JSCSSVision)"></script>
    <script type="text/javascript" src="/lib/dist/js/adminlte.min.js?ver=@(ViewBag.JSCSSVision )"></script>
    <script src="/lib/Scrollbar/jquery.slimscroll.min.js?ver=@(ViewBag.JSCSSVision )"></script>
    <script type="text/javascript" src="/lib/dist/js/app.min.js?ver=@(ViewBag.JSCSSVision)"></script>
    <script type="text/javascript" src="/lib/template/artTemplate.js?ver=@(ViewBag.JSCSSVision)"></script>
    <!--行内编辑-->
    <link href="/lib/bootstrap/editable/dist/bootstrap3-editable/css/bootstrap-editable.css?ver=@(ViewBag.JSCSSVision)" rel="stylesheet" />
    <script src="/lib/bootstrap/editable/dist/bootstrap3-editable/js/bootstrap-editable.js?ver=@(ViewBag.JSCSSVision)"></script>
    <!--行内编辑-->
    <!--Checked-->
    <link href="/lib/bootstrap/switch/dist/css/bootstrap3/bootstrap-switch.css?ver=@(ViewBag.JSCSSVision)" rel="stylesheet" />
    <script src="~/lib/bootstrap/switch/dist/js/bootstrap-switch.min.js"></script>

    <!--Checked-->
    <!--Layui-->
    <link href="/lib/common/css/layer-style.css" rel="stylesheet" />
    <script src="/lib/layer/layer.js?ver=@(ViewBag.JSCSSVision)"></script>
    <!--Layui-->
    <script src="/lib/common/js/X-edittable-ajax.js"></script>
    @{
        if (isHaveListMore)
        {
            <script src="/lib/common/js/LoadMore.js"></script>
        }
    }
    <script src="/lib/common/js/Toast.js"></script>
    <script src="/lib/common/js/SendAjax.js"></script>
</head>
<body class="hold-transition skin-blue fixed sidebar-mini">
    <div class="wrapper">
        <div class="container-header">
            <div class="pro-zc-bg"></div>
            <header class="main-header">
                <a href="javascript:;" class="logo">
                    <span class="logo-mini"><b>D</b>JWB</span>
                    <span class="logo-lg">
                        <span>
                            <b>JW</b>cms
                            <i class="fa fa-angle-down static-nav-icon" style="display:none"></i>
                        </span>
                    </span>
                </a>
                <ul class="static-nav dropdown-menu"></ul>
                <nav class="navbar navbar-static-top">
                    <a href="javascript:;" class="sidebar-toggle" data-toggle="push-menu" role="button">
                        <span class="sr-only">切换</span>
                    </a>

                    <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
                        <ul class="nav navbar-nav contacts-nav"></ul>

                    </div>
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">
                            <li>
                                <a href="javascript:;" title="点击查看" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </header>
        </div>

        @Html.Action("Left", "Layout")


        @RenderBody()
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> @(ViewBag.CMSVision)
            </div>
            <strong>Copyright © 2018 GVcms Soution.</strong> All rights
            reserved.
        </footer>

        <aside class="control-sidebar control-sidebar-dark" style="background-color:#fff !important">
            <div class="tab-content">
                <div id="control-sidebar-theme-demo-options-tab" class="tab-pane active">
                    <div class="tab-pane" id="control-sidebar-stats-tab">
                        <div class="box">
                            <div class="box-header with-border">
                                <h3 class="box-title">快捷区域</h3>
                                <div class="box-tools pull-right">

                                </div>
                            </div>
                            <div class="box-body">
                                <a href="javascript:;" class="btn btn-primary btn-block btn-flat"><i class="fa fa-delicious"></i>  查看主站</a>
                            </div>
                            <div class="box-body">
                                <a href="/cloudAdmin/custom/password.html" class="btn btn-primary btn-block btn-flat"><i class="fa fa-unlock"></i>  修改密码</a>
                            </div>

                            <div class="box-body">
                                <a href="/cloudAdmin/custom/log.html" class="btn btn-primary btn-block btn-flat"><i class="fa fa-file-text-o"></i>  查看日志</a>
                            </div>

                            <div class="box-body">
                                <a href="javascript:;" class="btn btn-primary btn-block btn-flat btn-close-login" onclick="cloudSoultionCore.CloseLogin()"><i class="fa fa-close"></i>  退出登录</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </aside>
        <div class="control-sidebar-bg"></div>
    </div>
</body>
</html>
